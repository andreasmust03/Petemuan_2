public class ContohMethodReturn {
    public static void main(String[] args) {
        int penjumlahan = jumlah(50,100);
        System.out.println(penjumlahan);

            }
    private static int jumlah(int angkaSatu, int angkaDua){
        int hasil = angkaDua + angkaSatu;
        return hasil;
    }
}
